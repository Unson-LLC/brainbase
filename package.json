{
  "name": "brainbase-unson",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "UNSON members' brainbase distribution with 89 Skills and 20 Commands",
  "repository": {
    "type": "git",
    "url": "https://github.com/Unson-LLC/brainbase-unson.git"
  },
  "license": "UNLICENSED",
  "author": "Unson LLC",
  "keywords": [
    "brainbase",
    "unson",
    "skills",
    "commands",
    "ai-pm",
    "private"
  ],
  "bugs": {
    "url": "https://github.com/Unson-LLC/brainbase-unson/issues"
  },
  "homepage": "https://github.com/Unson-LLC/brainbase-unson#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "prestart": "bash scripts/check-worktree.sh",
    "predev": "bash scripts/check-worktree.sh",
    "dev": "node --watch-path=server --watch-path=lib --watch-path=public --watch-path=server.js --watch-path=package.json --watch-preserve-output server.js",
    "start": "node server.js",
    "auth-setup": "node scripts/mcp-setup.mjs",
    "mcp-setup": "node scripts/mcp-setup.mjs",
    "mcp:add:brainbase": "node scripts/register-brainbase-mcp.mjs",
    "mcp:get:brainbase": "claude mcp get brainbase",
    "agent-browser:install": "npx -y agent-browser install",
    "agent-browser:help": "npx -y agent-browser --help",
    "test:server": "BRAINBASE_TEST_MODE=true node server.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:mobile": "playwright test --config=playwright-lambdatest.config.js tests/e2e/mobile-keyboard-scroll.spec.js",
    "test:mobile:iphone": "playwright test --config=playwright-lambdatest.config.js tests/e2e/mobile-keyboard-scroll.spec.js --project='iPhone 13 Pro - Emulated'",
    "test:mobile:android": "playwright test --config=playwright-lambdatest.config.js tests/e2e/mobile-keyboard-scroll.spec.js --project='Pixel 7 - Emulated'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.978.0",
    "@aws-sdk/credential-providers": "^3.978.0",
    "@aws-sdk/lib-dynamodb": "^3.978.0",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@octokit/rest": "^22.0.1",
    "chokidar": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "front-matter": "^4.0.2",
    "gray-matter": "^4.0.3",
    "http-proxy-middleware": "^3.0.5",
    "js-yaml": "^4.1.1",
    "jsonwebtoken": "^9.0.2",
    "marked": "^17.0.1",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "open": "^11.0.0",
    "pg": "^8.18.0",
    "ulid": "^2.4.0",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@vitest/coverage-v8": "^4.0.16",
    "jsdom": "^27.3.0",
    "supertest": "^7.1.4",
    "vitest": "^4.0.16"
  }
}
